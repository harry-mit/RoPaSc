<?xml version='1.0' encoding='utf-8'?>
<widget id="gr.developit.ropasc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <name>RoPaSc</name>
    <description>
        This game was made by Charalampos Mitrogeorgos for practise,
        during his internship on April 2019.
    </description>
    <author email="harrysmitrogeorgos@hotmail.com" href="www.developit.gr">
        Charalampos Mitrogeorgos
    </author>
    <icon src="\www\img\icon.png" />
    <content src="menu.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
