<?xml version='1.0' encoding='utf-8'?>
<widget id="gr.developit.ropasc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RoPaSc</name>
    <description>
        This game was made by Charalampos Mitrogeorgos for practise,
        during his internship on April 2019.
    </description>
    <author email="harrysmitrogeorgos@hotmail.com" href="www.developit.gr">
        Charalampos Mitrogeorgos
    </author>
    <icon src="\www\img\icon.png"/>
    <content src="menu.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
